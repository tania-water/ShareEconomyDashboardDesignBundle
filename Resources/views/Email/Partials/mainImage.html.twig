<table width="100%" cellspacing="0" cellpadding="0" border="0" bgcolor="#eeeeee" st-sortable="banner" id="backgroundTable">
    <tbody>
        <tr>
            <td align="center">
                <table width="600" cellspacing="0" cellpadding="0" border="0" bgcolor="#fcfcfc" align="center" class="devicewidth">
                    <tbody>

                        <tr>
                            <td width="100%" align="center">
                                <table width="600" cellspacing="0" cellpadding="0" border="0" align="center" class="devicewidth">
                                    <tbody>
                                        <tr>
                                            <!-- start of image -->
                                            <td align="center">
                                                <!-- logo -->
                                                <table width="600" cellspacing="0" cellpadding="0" border="0" bgcolor="#FCFCFC" align="left" class="devicewidth">
                                                    <tbody>
                                                        <tr>
                                                            <td width="600" height="60" align="center">
                                                                <div class="imgpop">
                                                                    <a href="" target="_blank">
                                                                        <img width="560" border="0" height="200" style="margin: 0 auto;display:block; border:none; outline:none; text-decoration:none;" alt="" src="{% if mainImageUrl is defined and mainImageUrl %}{{ mainImageUrl }}{% else %}{{ absolute_url(asset('/img/' ~ currentTheme ~ '/banner.png')) }}{% endif %}" class="intro">
                                                                    </a>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </tbody>
                                                </table>
                                                <!-- end of logo -->
                                            </td>

                                        </tr>
                                    </tbody>
                                </table>
                                <!-- end of image -->
                            </td>
                        </tr>

                    </tbody>
                </table>
            </td>
        </tr>
    </tbody>
</table>{# empty Twig template #}
